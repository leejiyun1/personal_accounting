# Database Configuration
POSTGRES_DB=personal_accounting
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PORT=6379

# Application Configuration
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/personal_accounting
SPRING_DATASOURCE_USERNAME=your_username
SPRING_DATASOURCE_PASSWORD=your_password
SPRING_REDIS_HOST=redis
SPRING_REDIS_PORT=6379

# Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_minimum_64_characters
JWT_ACCESS_TOKEN_VALIDITY=1800000
JWT_REFRESH_TOKEN_VALIDITY=604800000

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Profile
SPRING_PROFILES_ACTIVE=dev